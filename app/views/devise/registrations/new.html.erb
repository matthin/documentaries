<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= f.text_field(:name, label: 'Username', placeholder: 'Username') %>

  <%= f.email_field(:email, label: 'Email Address', placeholder: 'Email Address') %>

  <%= f.password_field(:password, placeholder: 'Password') %>

  <%= f.submit('Sign Up!') %>
<% end %>
